<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<Version>1.7.3</Version>
		<Description>图片处理（验证码、缩放、裁剪、压缩、二值化、旋转、翻转、添加图片、添加文字、水印）</Description>
		<Authors>Findx</Authors>
		<Company>Findx</Company>
		<LangVersion>10.0</LangVersion>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="fonts\actionj.ttf" />
		<None Remove="SixLabors.ImageSharp.Drawing" />
		<None Remove="SixLabors.ImageSharp" />
		<None Remove="fonts\Sketch-3D-1.otf" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="fonts\Actionj.ttf" />
		<EmbeddedResource Include="fonts\Sketch-3D-1.otf" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="SixLabors.ImageSharp.Drawing" Version="1.0.0-beta14" />
		<PackageReference Include="SixLabors.ImageSharp" Version="2.1.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Findx.Common\Findx.csproj" />
	</ItemGroup>

</Project>
