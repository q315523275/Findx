<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<Version>1.5.9</Version>
		<Description>图片处理（验证码、缩放、裁剪、压缩、二值化、旋转、翻转、添加图片、添加文字、水印）</Description>
		<Authors>Findx</Authors>
		<Company>Findx</Company>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="fonts\Breo.ttf" />
		<None Remove="fonts\actionj.ttf" />
		<None Remove="SixLabors.ImageSharp.Drawing" />
		<None Remove="SixLabors.ImageSharp" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="fonts\Breo.ttf" />
		<EmbeddedResource Include="fonts\actionj.ttf" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="SixLabors.ImageSharp.Drawing" Version="1.0.0-beta13" />
		<PackageReference Include="SixLabors.ImageSharp" Version="1.0.4" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Findx.Common\Findx.csproj" />
	</ItemGroup>

</Project>
